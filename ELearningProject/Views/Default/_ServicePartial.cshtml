@model List<ELearningProject.DAL.Entities.Service>

<!-- Service Start -->
<div class="container-xxl py-5">
    <div class="container">
        <div class="row g-4">
            <div class="col-lg-3 col-sm-6 wow fadeInUp" data-wow-delay="0.1s">
                <div class="service-item text-center pt-3">
                    <div class="p-4">
                        <i class="@Model[3].Icon text-primary mb-4"></i>
                        <h5 class="mb-3">@Model[3].Title</h5>
                        <p>@Model[3].Description</p>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-sm-6 wow fadeInUp" data-wow-delay="0.3s">
                <div class="service-item text-center pt-3">
                    <div class="p-4">
                        <i class="@Model[2].Icon text-primary mb-4"></i>
                        <h5 class="mb-3">@Model[2].Title</h5>
                        <p>@Model[2].Description</p>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-sm-6 wow fadeInUp" data-wow-delay="0.5s">
                <div class="service-item text-center pt-3">
                    <div class="p-4">
                        <i class="@Model[1].Icon text-primary mb-4"></i>
                        <h5 class="mb-3">@Model[1].Title</h5>
                        <p>@Model[1].Description</p>
                    </div>
                </div>
            </div>
            <div class="col-lg-3 col-sm-6 wow fadeInUp" data-wow-delay="0.7s">
                <div class="service-item text-center pt-3">
                    <div class="p-4">
                        <i class="@Model[0].Icon text-primary mb-4"></i>
                        <h5 class="mb-3">@Model[0].Title</h5>
                        <p>@Model[0].Description</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Service End -->

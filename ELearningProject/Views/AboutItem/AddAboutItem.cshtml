@model ELearningProject.DAL.Entities.AboutItem

@{
    ViewBag.Title = "AddAboutItem";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Yeni Hizmet Bilgileri</h3>
<form method="post">
    <br />
    <label>Hizmet Adı</label>
    <input type="text" name="ItemName" class="form-control" />
    <br />
    <br />
    <div class="form-check">
        <input class="form-check-input" type="checkbox" name="Status" value="true" id="status">
        <label class="form-check-label" for="status">Durum</label>
    </div>
    <br />
    <br />
    <button class="btn btn-primary">Hizmeti Kaydet</button>
</form>
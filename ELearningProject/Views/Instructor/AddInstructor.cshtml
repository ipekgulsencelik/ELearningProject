@model ELearningProject.DAL.Entities.Instructor

@{
    ViewBag.Title = "AddInstructor";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<h3>Yeni Eğitmen Bilgileri</h3>
<form method="post" enctype="multipart/form-data">
    <br />
    <label>Eğitmen Adı</label>
    <input type="text" name="Name" class="form-control" />
    <br />
    <br />
    <label>Eğitmen Soyadı</label>
    <input type="text" name="surname" class="form-control" />
    <br />
    <br />
    <label>Eğitmen Görseli</label>
    <input type="file" name="image" class="form-control"/>
    <br />
    <br />
    <button class="btn btn-primary">Eğitmeni Kaydet</button>
</form>